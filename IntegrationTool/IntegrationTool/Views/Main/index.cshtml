<!DOCTYPE html>
<html ng-app="IntegrationToolApp">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Integration Tool</title>
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

        <link rel="icon" type="image/png" href="../Content/Images/favicon.png" />
        @Styles.Render("~/Content/CSSBootstrap")

         <!-- Font Awesome -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
        <!-- Ionicons -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
        <!-- Sweet Alert -->
        <link rel="stylesheet" href="https://rawgit.com/t4t5/sweetalert/master/dist/sweetalert.css">

        @Styles.Render("~/Content/Styles")
        @Styles.Render("~/Content/CSSTemplate")


        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="hold-transition skin-blue sidebar-mini">
        
        <div ui-view></div>

        @Scripts.Render("~/Content/Plugins")
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.8/angular.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.8/angular-route.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular-messages/1.5.8/angular-messages.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.8/angular-cookies.min.js"></script>
        <script src="//rawgit.com/t4t5/sweetalert/master/dist/sweetalert.min.js"></script>
        <script src="//rawgit.com/tushariscoolster/ng-sweet-alert/master/ng-sweet-alert.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-calendar/1.0.0/calendar.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/2.1.3/ui-bootstrap.min.js"></script>

        @Scripts.Render("~/Content/moduleAngular")

        @Scripts.Render("~/Content/JSBootstrap")

        @Scripts.Render("~/Content/IntegrationToolAppControllers")

        @Scripts.Render("~/Content/IntegrationToolAppDirectives")

        @Scripts.Render("~/Content/IntegrationToolApp")

        @Scripts.Render("~/Content/IntegrationToolAppServices")
    </body>
</html>
