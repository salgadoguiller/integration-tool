<div>
  <br />
    <feedback type=typeMessage message=message></feedback>
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="col-md-10 col-md-offset-1">
       <h1>Configuration Reports</h1>
       <ol class="breadcrumb">
         <li class="breadcrumb-item"><i class="fa fa-cog"></i> Reports</li>
       </ol>
      </div>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <!-- /.col (left) -->
        <div class="col-md-10 col-md-offset-1">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Reports</h3>
            </div>
            <div class="box-body">
            
            <form role="form" ng-submit="sendRequest(request)"  novalidate>             
                              
             <div class="form-group">
                         
             <center>
               <div class="row">
                
                <div class="col-md-3">
                 <div class="form-group">                       
                  <select class="form-control" id="IntegrationType" name="IntegrationType" style="width: 100%;" ng-model="request.IntegrationType">
                   <option value="" selected disabled>Select Type Integration</option>
                    <option ng-repeat="integrationType in listIntegrationType" value="{{integrationType.IntegrationTypeId}}">{{integrationType.Name}}</option>                          
                  </select>
                 </div>
                </div>

                <div class="col-md-3">
                 <div class="form-group">                        
                  <select class="form-control" id="IntegrationCategory" name="IntegrationCategory" style="width: 100%;" ng-model="request.IntegrationCategory">
                   <option value="" selected disabled>Select Category Integration</option>
                   <option ng-repeat="integrationCategory in listIntegrationCategory" value="{{integrationCategory.IntegrationCategoryId}}">{{integrationCategory.Name}}</option>     
                  </select>
                 </div>
                </div>

                <div class="col-md-3">
                 <div class="form-group">                  
                  <select class="form-control" id="OperationWebServices" name="OperationWebServices" style="width: 100%;" ng-model="request.OperationWebServices">
                   <option value="" selected disabled>Select Operation Web Services</option>
                   <option ng-repeat="operationWebServices in  listOperationWebServices" value="{{operationWebServices.OperationWebServiceId}}">{{operationWebServices.Name}}</option>     
                  </select>
                 </div>
                </div>

                <div class="col-md-3">
                 <div class="form-group">                      
                  <select class="form-control" id="DatabaseParameter" name="DatabaseParameter" style="width: 100%;" ng-model="request.DatabaseParameter">
                   <option value="" selected disabled>Select Name Databases</option>
                   <option ng-repeat="databaseParameter in listDatabaseParameter" value="{{databaseParameter.EngineId}}">{{databaseParameter.Name}}</option>     
                  </select>
                 </div>
                </div>

               </div>               
             </center>
                                                                      
             </div>
               <!-- Date and time range -->
            <div class="form-group">
              <center>
                <div class="input-group">
                  <button type="button" class="btn btn-primary pull-left" id="daterange-btn"><span><i class="fa fa-calendar"></i> Date range picker</span><i class="fa fa-caret-down"></i></button>
                </div>
              </center>          
            </div>

            <div class="form-group">
              <center>
                <div class="input-group">                

                 <label class="radio-inline"><input type="radio" name="optradio" ng-model="Report.name" value="Excel" >Excel</label>
                 <label class="radio-inline"><input type="radio" name="optradio" ng-model="Report.name" value ="PDF">PDF</label>                 
                </div>
              </center>                           
            </div>
           
            <center>
              <div class="card-drop">
                <a class='toggle'><span class='label-active'>Select One Option...</span></a>
                <ul>
                  <li class='active'><a data-label="Integration Logs" value="Integration Logs" >Integration Logs</a></li>
                  <li><a data-label="System Logs" value="System Logs" >System Logs</a></li>
                </ul>
              </div>
            </center>

                     <br><br>
              <div class="box-footer" >                
               <center>
                 <button type="submit" class="btn btn-primary" id="submit" data-loading-text="Loading..." autocomplete="off">Generate</button>
               </center>
              </div>  
            </form>
            </div>

                
              <!-- /.form group -->
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col (right) -->
    </section>
    <!-- /.content -->
    </div>

<script>

    $(document).ready(function () {
        
    });
    

</script>