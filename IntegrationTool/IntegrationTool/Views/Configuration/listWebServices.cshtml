<div>
    <br />
    <feedback type=typeMessage message=message></feedback>

    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="col-md-10 col-md-offset-1">
        <h1>
            List Web Services
        </h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><i class="fa fa-cog"></i> Configuration</li>
            <li class="breadcrumb-item active"> List Web Services</li>
        </ol>
        </div>
    </section>
    
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-md-10 col-md-offset-1">


          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Web Services</h3>
              <a href="/#/main/configuration/formWebService/-1" class="pull-right btn btn-xs btn-success">Add</a>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive">
              <table id="datatable" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Endpoint</th>
                  <th>Username</th>
                  <th>Password</th>
                  <th>Options</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-repeat="webService in listWebServices">
                    <td>{{webService.Endpoint}}</td>
                    <td>{{webService.Username}}</td>
                    <td>{{webService.Password}}</td>
                    <td>
                        <div ng-controller="AlertController">
                            <button class="btn btn-xs btn-success" ng-click="editWebService(webService.WebServiceId)">Edit</button> 
                            <button class="btn btn-xs btn-danger" sweetalert sweet-options="{{sweet.option}}" sweet-on-confirm="deleteWebService(webService.WebServiceId)" sweet-on-cancel="return">Delete</button>
                        </div>
                    </td>
                </tr>
                </tbody>
                <tfoot>
                <tr>
                  <th>Endpoint</th>
                  <th>Username</th>
                  <th>Password</th>
                  <th>Options</th>
                </tr>
                </tfoot>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
</div>