<div>
    <br />
    <feedback type=typeMessage message=message></feedback>

    <div ng-show="isloading" id="Preloader"></div>
    <div ng-if="!isloading">

    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="col-md-10 col-md-offset-1">
        <h1>
            List Keys
        </h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><i class="fa fa-cog"></i> Configuration</li>
            <li class="breadcrumb-item active"> List Keys</li>
        </ol>
        </div>
    </section>
    
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-md-10 col-md-offset-1">


          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Keys</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive">
              <table id="datatable" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Key</th>
                  <th>Description</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-repeat="key in listKeys">
                    <td>{{key.KeyValue}}</td>
                    <td>{{key.Description}}</td>
                </tr>
                </tbody>
                <tfoot>
                <tr>
                    <th>Key</th>
                    <th>Description</th>
                </tr>
                </tfoot>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
</div>