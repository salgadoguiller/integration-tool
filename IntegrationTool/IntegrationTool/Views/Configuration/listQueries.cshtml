<div>
    <br />
    <feedback type=typeMessage message=message></feedback>

    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="col-md-10 col-md-offset-1">
        <h1>
            List Queries
        </h1>
        <ol class="breadcrumb">
            <li><a><i class="fa fa-dashboard"></i> Configuration</a></li>
            <li class="active">List Queries</li>
        </ol>
        </div>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-md-10 col-md-offset-1">

          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Queries</h3>
                <a href="/#/Configuration/formQuery" class="pull-right btn btn-xs btn-success">Add</a>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive">
                <table id="datable" class="table table-bordered table-striped table-hover">                    
                    <thead>
                    <tr >
                      <th>Query Type</th>
                      <th>Query</th>
                      <th>Options</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="query in listQueries">
                       <td>{{query.QueriesType.Name}}</td>
                       <td>{{query.Query1}}</td>
                       <td>
                            <div ng-controller="AlertController">
                                <button class="btn btn-xs btn-success"">Edit</button> 
                                <button class="btn btn-xs btn-danger" sweetalert sweet-options="{{sweet.option}}" sweet-on-confirm="deleteQuery(query.QueryId)" sweet-on-cancel="return">Delete</button>
                            </div>
                       </td>
                    </tr>
                    </tbody>
                    <tfoot>
                    <tr>
                        <th>Query Type</th>
                        <th>Query</th>
                        <th>Options</th>
                    </tr>
                    </tfoot>
                </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
</div>