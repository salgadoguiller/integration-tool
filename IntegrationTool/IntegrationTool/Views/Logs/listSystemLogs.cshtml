<div>
    <br />
    <feedback type=typeMessage message=message></feedback>

    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="col-md-10 col-md-offset-1">
        <h1>
            List System Logs
        </h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><i class="fa fa-cog"></i> Logs</li>
            <li class="breadcrumb-item active"> List System Logs</li>
        </ol>
        </div>
    </section> 
    
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-md-10 col-md-offset-1">


          <div class="box">
            <div class="box-header">
              <h3 class="box-title">System Logs</h3>                
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Description</th>               
                  <th>Integration Name</th>
                  <th>Error Date</th>
                </tr>
                </thead>
                <tbody>

                <tr ng-repeat="systemLogs in listSystemLogs">
                  <td>{{systemLogs.Description}}</td>
                  <td>{{systemLogs.Integration.IntegrationName}}</td>                  
                  <td>{{systemLogs.ErrorDate}}</td>                
                </tr>
                </tbody>
                <tfoot>
                <tr>
                  <th>Description</th>                
                  <th>Integration Name</th>
                  <th>Error Date</th>
                </tr>
                </tfoot>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
</div>



