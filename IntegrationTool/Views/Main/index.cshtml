<!DOCTYPE html>
<html ng-app="IntegrationToolApp">
    <head>
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <title>Integration Tool</title>
      <!-- Tell the browser to be responsive to screen width -->
      <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <link rel="icon" type="image/png" href="../Content/Images/favicon.png" />
      
      @Styles.Render("~/Content/CSSBootstrap")
      <!-- Font Awesome -->
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
      <!-- Ionicons -->
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
      
      @Styles.Render("~/Content/CSSTemplate")
      @Styles.Render("~/Content/CSSBootstrap")


      <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
      <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
      <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
      <![endif]-->
    </head>
    <body class="hold-transition skin-blue sidebar-mini">
        <!-- Site wrapper -->
        <div class="wrapper">

          <header class="main-header">
            <!-- Logo -->
            <a href="index2.html" class="logo">
              <!-- mini logo for sidebar mini 50x50 pixels -->
              <span class="logo-mini"><b>I</b>Tool</span>
              <!-- logo for regular state and mobile devices -->
              <span class="logo-lg"><b>Integration</b>Tool</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
              <!-- Sidebar toggle button-->
              <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </a>

              <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">
                  <!-- User Account: style can be found in dropdown.less -->
                  <li class="dropdown user user-menu">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                      <img src="../../Content/Images/Icon-user.png" class="user-image" alt="User Image">
                      <span class="hidden-xs">User-Laureate</span>
                    </a>
                    <ul class="dropdown-menu">
                      <!-- User image -->
                      <li class="user-header">
                        <img src="../../Content/Images/Icon-user.png" class="img-circle" alt="User Image">

                        <p>
                          User-Laureate
                          <small>Administrator</small>
                        </p>
                      </li>

                      <!-- Menu Footer-->
                      <li class="user-footer">
                        <div class="pull-left">
                          <a href="#" class="btn btn-default btn-flat">Profile</a>
                        </div>
                        <div class="pull-right">
                          <a href="#" class="btn btn-default btn-flat">Sign out</a>
                        </div>
                      </li>
                    </ul>
                  </li>
                  <!-- Control Sidebar Toggle Button -->
                  <li>
                    <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                  </li>
                </ul>
              </div>
            </nav>
          </header>

          <!-- =============================================== -->

          <!-- Left side column. contains the sidebar -->
          <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
              <!-- Sidebar user panel -->
              <div class="user-panel">
                <div class="pull-left image">
                  <img src="../../Content/Images/Icon-user.png" class="img-circle" alt="User Image">
                </div>
                <div class="pull-left info">
                  <p>User-Laureate</p>
                  <a href="#"><i class="fa fa-circle text-success"></i>Administrator</a>
                </div>
              </div>

              <!-- sidebar menu: : style can be found in sidebar.less -->
              <ul class="sidebar-menu">
                <li class="header">Options</li>
                <li class="treeview">
                  <a href="/#/main">
                    <i class="fa fa-dashboard"></i> <span>Main</span>
                  </a>
                </li>
                <li class="treeview">
                  <a>
                    <i class="fa fa-cog"></i>
                    <span>Configuration</span>
                    <span class="pull-right-container">
                      <i class="fa fa-angle-left pull-right"></i>
                    </span>
                  </a>
                  <ul class="treeview-menu">
                    <li><a href="/#/Configuration/activedirectory"><i class="fa fa-circle-o"></i>Active Directory</a></li>
                    <li><a href="/#/Configuration/serversmtp"><i class="fa fa-circle-o"></i>Server SMTP</a></li>
                    <li><a href="/#/Configuration/databases"><i class="fa fa-circle-o"></i>Databases</a></li>
                    <li><a href="/#/Configuration/webservices"><i class="fa fa-circle-o"></i>Web Services</a></li>
                    <li><a href="/#/Configuration/flatfiles"><i class="fa fa-circle-o"></i>Flat Files</a></li>
                    <li><a href="/#/Configuration/queries"><i class="fa fa-circle-o"></i>Queries</a></li>
                    <li><a href="/#/Configuration/headers"><i class="fa fa-circle-o"></i>Headers</a></li>
                  </ul>
                </li>
                <li class="treeview">
                  <a href="#">
                    <i class="fa fa-files-o"></i>
                    <span>Integrations</span>
                    <span class="pull-right-container">
                      <i class="fa fa-angle-left pull-right"></i>
                    </span>
                  </a>
                  <ul class="treeview-menu">
                    <li><a href="#"><i class="fa fa-circle-o"></i>Automatic</a></li>
                    <li><a href="#"><i class="fa fa-circle-o"></i>Manual</a></li>
                  </ul>
                </li>

                <li class="treeview">
                  <a href="#">
                    <i class="fa fa-users"></i>
                    <span>Users</span>
                    <span class="pull-right-container">
                      <i class="fa fa-angle-left pull-right"></i>
                    </span>
                  </a>
                  <ul class="treeview-menu">
                    <li><a href="#"><i class="fa fa-circle-o"></i>Locals</a></li>
                    <li><a href="#"><i class="fa fa-circle-o"></i>Active Directory</a></li>
                  </ul>
                </li>
                <li>
                  <a href="#">
                    <i class="fa fa-file"></i> <span>Reports</span>
                  </a>
                </li>

                <li class="treeview">
                  <a href="#">
                    <i class="fa fa-laptop"></i>
                    <span>Logs</span>
                    <span class="pull-right-container">
                      <i class="fa fa-angle-left pull-right"></i>
                    </span>
                  </a>
                  <ul class="treeview-menu">
                    <li><a href="#"><i class="fa fa-circle-o"></i>Integrations</a></li>
                    <li><a href="#"><i class="fa fa-circle-o"></i>System</a></li>
                  </ul>
                </li>
                <li><a href="#"><i class="fa fa-book"></i> <span>Documentation</span></a></li>
              </ul>
            </section>
            <!-- /.sidebar -->
          </aside>

          <!-- =============================================== -->

          <!-- Content Wrapper. Contains page content -->
          <div class="content-wrapper">
            <div ng-view></div>
          </div>
          <!-- /.content-wrapper -->

          <footer class="main-footer">
            <strong>Copyright &copy;<a href="http://lno.laureate.net/"> Laureate Network Office</a>.</strong> All rights
            reserved.
          </footer>

          <!-- Control Sidebar -->
          <aside class="control-sidebar control-sidebar-dark">
            <!-- Tab panes -->
            <div class="tab-content">
              <!-- Home tab content -->
              <div class="tab-pane" id="control-sidebar-home-tab">
              </div>
            </div>
          </aside>
          <!-- /.control-sidebar -->
          <!-- Add the sidebar's background. This div must be placed
               immediately after the control sidebar -->
        </div>
        <!-- ./wrapper -->

        <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.20/angular.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.20/angular-route.min.js"></script>

        @Scripts.Render("~/Content/Plugins")

        @Scripts.Render("~/Content/JSBootstrap")

        @Scripts.Render("~/Content/JSTemplate")

        @Scripts.Render("~/Content/IntegrationToolApp")
    </body>
</html>